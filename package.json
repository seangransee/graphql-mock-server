    "build": "tsc",
    "test": "jest",
    "watch": "tsc --watch"
  },
  "files": [
    "lib"
  ],
  "repository": "git@github.com:seangransee/graphql-yaml-mocks.git",
  "author": "Sean Gransee <sean.gransee@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.3",
    "@types/graphql": "^14.0.0",
    "@types/jest": "^23.3.2",
    "@types/js-yaml": "^3.11.2",
    "@types/node": "^10.9.4",
    "graphql": "^14.0.2",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3"
  },
  "jest": {
